{
  "name": "postcss-dynamic-value",
  "version": "0.0.6",
  "description": "PostCSS plugin that implements input actual value selectors",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "postcss-dynamic-value"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "author": "Mauro Bartolomeoli <maurobartolomeoli@gmail.com>",
  "license": "MIT",
  "repository": "mbarto/postcss-dynamic-value",
  "engines": {
    "node": ">=12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "postcss": "^8.3.11",
    "prettier": "^2.2.1",
    "rollup": "^3.10.1",
    "vitest": "^0.27.2"
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
